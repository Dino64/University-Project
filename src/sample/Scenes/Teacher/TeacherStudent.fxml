      <Button fx:id="addGrade" layoutX="497.0" layoutY="329.0" mnemonicParsing="false" text="add Grade" />
      <Button fx:id="backTeach" layoutY="2.0" mnemonicParsing="false" onAction="#backButton" text="&lt;&lt;back" />
