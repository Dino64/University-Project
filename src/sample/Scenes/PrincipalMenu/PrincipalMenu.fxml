<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Scenes.PrincipalMenu.PrincipalController">
   <children>
      <ImageView fx:id="PrincipalImageView" fitHeight="663.0" fitWidth="602.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Images/campus.sommar.jpg" />
         </image>
      </ImageView>
      <Button fx:id="RoomMenuButton" layoutX="101.0" layoutY="207.0" mnemonicParsing="false" onAction="#pressRoomMenu" prefHeight="31.0" prefWidth="100.0" text="Room Menu" />
      <Button fx:id="ViewTeachersButton" layoutX="251.0" layoutY="207.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="100.0" text="View Teachers" />
      <Button fx:id="ViewStudentButton" layoutX="401.0" layoutY="207.0" mnemonicParsing="false" onAction="#pressViewStudent" prefHeight="31.0" prefWidth="100.0" text="View Student " />
      <Button fx:id="logOutButton" layoutX="255.0" layoutY="360.0" mnemonicParsing="false" onAction="#logOutButton" prefHeight="20.0" prefWidth="93.0" text="Log Out" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="201.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../Desktop/KaosUniversity.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
