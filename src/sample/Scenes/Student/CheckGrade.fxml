<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Scenes.Student.CheckGradeController">
   <children>
      <ImageView fitHeight="401.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Images/Library.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backButton" alignment="BOTTOM_LEFT" layoutY="-3.0" mnemonicParsing="false" onAction="#pressBackButton" prefHeight="31.0" prefWidth="600.0" text="Back" />
      <TextArea fx:id="ShowGradesTextArea" layoutX="192.0" layoutY="54.0" prefHeight="252.0" prefWidth="366.0" />
      <Button fx:id="checkMyGrade" layoutX="192.0" layoutY="325.0" mnemonicParsing="false" onAction="#pressCheckGrade" prefHeight="27.0" prefWidth="366.0" text="Check My Grade" />
   </children>
</AnchorPane>
