<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Scenes.Student.RegisterNewCourseController">
   <children>
      <ImageView fitHeight="401.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Images/campus.sommar.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backButton" alignment="BOTTOM_LEFT" layoutY="-1.0" mnemonicParsing="false" onAction="#pressBackButton" prefHeight="31.0" prefWidth="600.0" text="Back " />
      <Button fx:id="ShowCourseButton" layoutX="116.0" layoutY="337.0" mnemonicParsing="false" onAction="#pressShowCourse" prefHeight="27.0" prefWidth="207.0" text="Show Course" />
      <Button fx:id="registerCourseButton" layoutX="357.0" layoutY="337.0" mnemonicParsing="false" onAction="#pressRegisterCourse" prefHeight="27.0" prefWidth="207.0" text="Register Course" />
      <TableView fx:id="showCourse" layoutX="116.0" layoutY="48.0" prefHeight="243.0" prefWidth="445.0">
        <columns>
          <TableColumn fx:id="courseName" prefWidth="103.0" text=" Course Name" />
          <TableColumn fx:id="subject" minWidth="8.0" prefWidth="125.0" text="Subject" />
            <TableColumn fx:id="Grade" prefWidth="121.0" text="Grade" />
            <TableColumn fx:id="isRegistered" prefWidth="95.0" text="Course ID" />
        </columns>
      </TableView>
      <TextField fx:id="CourseName" layoutX="357.0" layoutY="300.0" prefHeight="27.0" prefWidth="207.0" promptText="Course Name" />
   </children>
</AnchorPane>
