<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="395.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Scenes.Student.StudentController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Images/campus.sommar.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" layoutX="367.0" layoutY="296.0" mnemonicParsing="false" onAction="#pressBackButton" prefHeight="32.0" prefWidth="160.0" text="Log out" textAlignment="CENTER" />
      <Button fx:id="CheckGradeButton" layoutX="367.0" layoutY="149.0" mnemonicParsing="false" onAction="#pressCheckGrade" prefHeight="27.0" prefWidth="160.0" text="Check My Grade" />
      <Button fx:id="RegisterNewCourse" layoutX="367.0" layoutY="227.0" mnemonicParsing="false" onAction="#pressRegisterNewCourse" prefHeight="27.0" prefWidth="160.0" text="Register New Course" />
      <Button fx:id="bookClassroomButton" layoutX="367.0" layoutY="81.0" mnemonicParsing="false" onAction="#pressBookClassroom" prefHeight="27.0" prefWidth="160.0" text="Book Classroom" />
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Images/KaosUniversity.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
