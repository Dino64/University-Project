<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Scenes.Student.StudentController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Assets/Images/Library.jpg" />
         </image>
      </ImageView>
      <Button fx:id="studentPageButton" layoutX="255.0" layoutY="302.0" mnemonicParsing="false" onAction="#studentPageButton" prefHeight="22.0" prefWidth="88.0" text="My page" />
      <Button fx:id="backButton" alignment="BOTTOM_LEFT" layoutY="-4.0" mnemonicParsing="false" onAction="#pressBackButton" prefHeight="32.0" prefWidth="600.0" text="Back" />
   </children>
</AnchorPane>
