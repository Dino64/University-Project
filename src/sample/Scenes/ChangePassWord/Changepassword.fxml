<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Scenes.ChangePassWord.ChangePassWord">
   <children>
      <Button fx:id="generateBtn" layoutX="56.0" layoutY="270.0" mnemonicParsing="false" onAction="#generateButton" text="generate PassWord" />
      <TextField fx:id="emailTxt" layoutX="56.0" layoutY="229.0" promptText="Enter Email" />
      <TextField fx:id="newPassTxt" layoutX="391.0" layoutY="270.0" promptText="New PassWord" />
      <TextField fx:id="recText" layoutX="391.0" layoutY="229.0" promptText="enter new code" />
      <Button fx:id="newPassBtn" layoutX="391.0" layoutY="305.0" mnemonicParsing="false" onAction="#newPassButton" text="save new Pass" />
      <Button fx:id="backBtn" layoutY="1.0" mnemonicParsing="false" onAction="#BackToLogin" prefHeight="26.0" prefWidth="71.0" text="&lt;&lt;Back" />
   </children>
</AnchorPane>
